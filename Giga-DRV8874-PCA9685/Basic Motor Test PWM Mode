#include <Adafruit_PWMServoDriver.h>

#define PinIN1 0
#define PinIN2 1
#define PinPmode 22
#define PinSleep 23


void setup(){
	pinMode(PinPmode,OUT);
	Adafruit_PWMServoDriver pca = Adafruit_PWMServoDriver(0x40);
	pca.begin();
	pca.setPWMFreq(500); //GIGA default

	digitalWrite(PinPmode,HIGH);
	delay(500);
	digitalWrite(PinPmode,HIGH);
}

void loop(){
	pca.setPin(PinPH,0);
	pca.setPin(PinEN,4095);
}